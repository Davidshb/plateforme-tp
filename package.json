  "main": "index.js",
  "scripts": {
    "server": "nodemon -w src/ -i src/client bin/server.js",
    "client": "node bin/client.js",
    "redis": "docker run -i --rm --name redis -p 6379:6379 redis",
    "rabbitmq_docker": "docker run -it --rm --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:3-management",
    "router": "nodemon -w src/router -w src/common -w src/db bin/router.js",
    "backend": "nodemon -w src/backend -w src/common -w src/db bin/backend.js",
    "display": "nodemon -w src/display -w src/common -w src/db bin/display.js",
    "rest": "nodemon -w src/rest -w src/common bin/rest.js",
    "ptr": "nodemon -w src/publishToRedis -w src/common -w src/db bin/publish_to_redis.js"
