{
  "loginSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "login": {
        "type": "string"
      },
      "password": {
        "type": "string"
      }
    },
    "required": [
      "login",
      "password"
    ]
  },
  "pushDataSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "token": {
        "type": "string"
      }
    },
    "required": [
      "token"
    ]
  }
}
